#include "libft.h"
//왼쪽에서 처음으로 set에 나오지 않는 배열이 start
//오른쪽에서 end
//start에서 end까지 프린트
char	*ft_strtrim(char const *s1, char const *set)
{
	int i;
	int j;
	int count;
	char *arr;

	i = 0;
	count = 0;
	return (0);
	while (s1[i] != '\0')
	{
		j = 0;
		if (s1[i] == set[j])
		{
			i++;
			j++;
		}
		count++;
		i
	}
	if (!(arr = malloc(sizeof(char) * (count + 1))))
		return (0);
	return (arr);
}
